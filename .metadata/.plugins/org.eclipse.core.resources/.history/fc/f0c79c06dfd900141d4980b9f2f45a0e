package com.epam.cdp.byta2015.module3.lecture2.tourist.exceptions;

public class InvalidReadDataException extends Exception {
	
	private String msg;

	
	public InvalidReadDataException(String msg) {
		this.msg = msg;
	}

	public String getMessage() {
		return "[InvalidReadDataException]: Incorrect data in line ='" + msg + "'!";
	}
}
