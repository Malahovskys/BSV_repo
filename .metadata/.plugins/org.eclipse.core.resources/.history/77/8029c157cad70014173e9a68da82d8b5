package com.epam.cdp.byta2015.module3.lecture2.tourist.comparators;

import java.util.Comparator;
import com.epam.cdp.byta2015.module3.lecture2.tourist.model.Offer;

public class SortedByType implements Comparator<Offer> {
	
    public int compare(Offer obj1, Offer obj2) {
        
        String str1 = obj1.getTypeofTour();
        String str2 = obj2.getTypeofTour();
       
        return str1.compareTo(str2);
  }
}
